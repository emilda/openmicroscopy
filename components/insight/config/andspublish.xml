<?xml version="1.0" encoding="utf-8"?>
<agent name="Register with RDA">
   <resources>
     <iconFactories>
	<!-- This entry is turned into an instance of: 
       org.openmicroscopy.shoola.env.config.IconFactory
       This object can then be used to retrieve any image file within
       the directory pointed by the location tag. -->
       <structuredEntry name="/resources/icons/Factory" type = "icons">
         <!-- The location tag specifies the FQN of the package that contains
         the icon files. -->
         <location>org.openmicroscopy.shoola.agents.monash.graphx</location>
       </structuredEntry>
     </iconFactories>
     
     <fonts>
       <!-- This entry is turned into an instance of java.awt.Font. --> 
       <structuredEntry name="/resources/fonts/Titles" type="font">
         <family>SansSerif</family>
         <size>12</size>
         <style>bold</style>
       </structuredEntry>
     </fonts>
</resources>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
   * General set up for the data manager
  ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<entry name="/services/monash/loginToken">http://localhost:8080/omeromd/user/userLogin.jspx</entry>
<entry name="/services/monash/mdregToken">http://localhost:8080/omeromd/data/mdReg.jspx</entry>
<entry name="/services/monash/partyToken">http://localhost:8080/omeromd/data/searchParty.jspx</entry>
</agent>